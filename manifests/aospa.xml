<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Manifest -->
  <project path="manifest" name="AOSPA/manifest" remote="aospa" groups="aospa" />

  <!-- Build repositories -->
  <remove-project name="platform/build" />
  <project path="build" name="AOSPA/android_build" remote="aospa" groups="aospa" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>

  <!-- Hardware -->
  <remove-project name="device/qcom/common" />
  <project path="device/qcom/common" name="AOSPA/android_device_qcom_common" remote="aospa" groups="pdk-cw-fs,aospa" />
